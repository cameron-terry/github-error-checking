{
  "name": "github-error-checking",
  "version": "0.1.0",
  "description": "GitHub Action to analyze pull requests for proper error handling",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build lib/index.js -o dist",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronterry/github-error-checking.git"
  },
  "keywords": [
    "github",
    "actions",
    "error-handling",
    "code-quality"
  ],
  "author": "Cameron Terry",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "parse-diff": "^0.11.1"
  },
  "devDependencies": {
    "@octokit/rest": "^19.0.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
